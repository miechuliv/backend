<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
			{% stylesheets
				"@AppBundle/Resources/public/js/jquery-ui-1.11.4/jquery-ui.css"
				
			%}
			<link type="text/css" rel="stylesheet" media="all" href="{{ asset_url }}" />
			{% endstylesheets %}
		{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
			{% javascripts
			'@AppBundle/Resources/public/js/jquery-1.11.3.min.js'
			'@AppBundle/Resources/public/js/jquery-ui-1.11.4/jquery-ui.min.js'
			
			%}
				<script type="text/javascript" src="{{ asset_url }}"></script>
			{% endjavascripts %}
		{% endblock %}
		
		
    </body>
</html>
